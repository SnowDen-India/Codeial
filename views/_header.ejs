
 <header>
    	 
    <div id="header">

      <div id="logo">
          <h1>codeial</h1>
          
      </div>
      <% if(locals.user){%>
       <div id="name-id">
           
           <li>
               <a href="/users/profile/<%=user.id%>">
                    <h3><%=user.name%></h3>
               </a>
           </li>
            
          <li>
              <a href="/users/sign-out">
                   <h3>Sign Out</h3>
              </a>
          </li>

       </div>

       <% }else{ %>
        <div id="name-id">
       <li>
        <a href="/users/sign-in">
          <h3>Sign In</h3>
       </a>
     </li>
    
  <li>
      <a href="/users/sign-up">
           <h3>Sign Up</h3>
      </a>
  </li>
</div>
        <% } %>
    </div>






</header>














